#docker
docker-compose build --no-cache
docker-compose up   
   
#Add-Migration
dotnet ef --startup-project services/RelatedProductsApi.Service migrations add InitialMigration --project src/RelatedProductsApi

#Update-Migration
dotnet ef --startup-project services/RelatedProductsApi.Service database update InitialMigration --project src/RelatedProductsApi

#Remove-Migration
dotnet ef --startup-project services/RelatedProductsApi.Service migrations remove --project src/RelatedProductsApi -f